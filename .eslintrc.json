{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": [
    "plugin:react/recommended",
    "xo"
  ],
  "overrides": [
  ],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "plugins": [
    "react"
  ],
  "rules": {
    "eol-last": [
      "error",
      "always"
    ],
    "capitalized-comments": "off",
    "react/prop-types": 1,
    "no-trailing-spaces": 0,
    "no-unused-vars": "off"
    //TODO: remove "no-unused-vars" when merge to main
    // 'max-param': 'off',
    // 'max-params': [
    // 	'error',
    // 	6,
    // ],
  }
}
